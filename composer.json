{
	"name": "customiesdevs/customies",
	"description": "Creates Custom Items & Blocks in Pocketmine-MP",
	"version": "1.4.0",
	"type": "library",
	"license": "MIT",
	"minimum-stability": "dev",
	"prefer-stable": true,
	"autoload": {
		"classmap": ["src"]
	},
	"scripts": {
		"build-phar": [
			"@composer install --no-dev --classmap-authoritative --ignore-platform-reqs",
			"php -dphar.readonly=0 vendor/bin/pharynx -i=. -c -p=Customies.phar"
		]
	},
	"config": {
		"sort-packages": true,
		"allow-plugins": {
			"phpstan/extension-installer": true
		}
	}
}